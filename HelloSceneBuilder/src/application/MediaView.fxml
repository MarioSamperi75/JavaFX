<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller_MediaVIew">
   <children>
      <AnchorPane prefHeight="620.0" prefWidth="320.0">
         <children>
            <Button fx:id="playButton" layoutX="69.0" layoutY="580.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
            <Button fx:id="pauseButton" layoutX="133.0" layoutY="580.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
            <Button fx:id="resetButton" layoutX="203.0" layoutY="580.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
            <MediaView fx:id="myMediaView" fitHeight="568.0" fitWidth="320.0" layoutY="4.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
